{
    "manifest_version": 3,
    "name": "Yumazzo",
    "short_name": "yumazzo",
    "version": "0.1.0",
    "description": "Final Round Diffusion Front End Engineering Challenge",
    "icons": {
        "16": "/icons/icon_16.png",
        "32": "/icons/icon_32.png",
        "48": "/icons/icon_48.png",
        "128": "/icons/icon_128.png",
        "256": "/icons/icon_256.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*",
                "http://*/*"
            ],
            "js": [
                "inject.js"
            ]
        }
    ],
    "action": {
        "default_popup": "index.html"
    }
}